        "S1.add(55)\n",
        "S1.add(66)\n",
        "print(\"S1 is--> \", S1)\n",
        "\n",
        "#(ii)\n",
        "S1.remove(10)\n",
        "S1.remove(30)\n",
        "print(\"S1 is--> \", S1)\n",
        "\n",
        "#(iii)\n",
        "print(\"is 40 in S1--> \", 40 in S1)\n",
        "\n",
        "#(iv)\n",
        "print(\"union between s1 and s2--> \", S1.union(S2))\n",
        "\n",
        "#(v)\n",
        "print(\"intersection is--> \", S1.intersection(S2))\n",
        "\n",
        "#(vi)\n",
        "print(\"S1-S2--> \", S1-S2)"
