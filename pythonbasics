        "#(i)\n",
        "import string as s\n",
        "import random as r\n",
        "\n",
        "for i in range (0,100):\n",
        "  k=r.randint(6,8)\n",
        "  pwd=r.sample(s.ascii_letters, k)\n",
        "  print(pwd)\n",
        "\n",
        "#(ii)\n",
        "def IsPrime(n):\n",
        "\tfor i in range(2, n//2 + 1):\n",
        "\t\tif n%i==0:\n",
        "\t\t\treturn 0\n",
        "\treturn 1\n",
        "\n",
        "for j in range(600, 800):\n",
        "  if IsPrime(j)==1:\n",
        "    print(j)\n",
        "\n",
        "#(iii)\n",
        "def div(n):\n",
        "  if(n%7==0 and n%9==0):\n",
        "    return 1\n",
        "  return 0\n",
        "\n",
        "for j in range(100, 1000):\n",
        "  if div(j)==1:\n",
        "    print(j)"
